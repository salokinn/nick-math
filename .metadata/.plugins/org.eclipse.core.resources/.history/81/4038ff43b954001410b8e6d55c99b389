//This class will create a basic calculator that can add, subtract, multiply, and divide numbers.


/* 
 */


package basic_calc;

//main class
public class Calculator {

	//main function
	public static void main(String[] args) {
		
		//Multiplication
		int a = 0;
		int b = 2;
		int c = 3;
		System.out.println("ab = " + a*b);
		System.out.println("bc = " + b*c);
		System.out.println("ac = " + a*c);
		
		//Division
		int d = 2;
		int e = 4;
		int f = 8;
		System.out.println("e/d = " + e/d);
		System.out.println("f/d =" + f/d);//TODO
		System.out.println("f/e = " + f/e);
		
	}
	
}
